<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        <!-- Axentix CSS minified version -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.1.0/dist/css/axentix.min.css">
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('css/style.css') }}">
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}
        <!-- Axentix JS minified version -->
        <script src="https://cdn.jsdelivr.net/npm/axentix@1.1.0/dist/js/axentix.min.js"></script>
    </head>
    <body>

    <header>
        <nav class="navbar shadow-1 primary">
            <a href="#" target="_blank" class="navbar-brand">Axentix</a>
            <div class="navbar-menu ml-auto">
                <a class="navbar-link" href="#">Link 1</a>
                <a class="navbar-link" href="#">Link 2</a>
                <a class="navbar-link" href="#">Link 3</a>
                <a class="navbar-link" href="#">Link 4</a>
            </div>
        </nav>
    </header>

    <main>
        <!-- Code here the main content -->
    </main>
    {% block body %}{% endblock %}

    <footer class="footer primary">
        Copyright Â© 2020 - Example
    </footer>

    <script src="https://cdn.jsdelivr.net/npm/axentix@1.1.0/dist/js/axentix.min.js">
    </script>

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    <script src="{{ asset('js/script.js') }}"></script>
    </body>
</html>
